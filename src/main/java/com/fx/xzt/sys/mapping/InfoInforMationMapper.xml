  <!--获取满足条件的咨询信息 -tianliya-start-->
  <select id="getByAll" resultType="java.util.Map" parameterType="java.util.Map" >
  	select @rownum:=@rownum+1 as rowNum,info_id as infoId, title, state, createtime, releasetime, operator
    from (select @rownum:=0)r,info_information
    <where>
	    <if test="title != null and title !='' ">
	    	and title like concat(concat('%',#{title,jdbcType=VARCHAR}),'%')
	    </if>
	    <if test="startTime !=null and startTime !='' ">
			and releasetime &gt;=#{startTime,jdbcType=TIMESTAMP}
		</if>
		<if test="endTime !=null and endTime !='' ">
			and releasetime &lt;=#{endTime,jdbcType=TIMESTAMP}
		</if>
		<if test="state != null">
			and state = #{state,jdbcType=SMALLINT}
		</if>
		<if test="operator != null and operator !='' ">
			and operator = #{operator,jdbcType=VARCHAR}
		</if>
    </where>
    order by  releasetime desc
  </select>
