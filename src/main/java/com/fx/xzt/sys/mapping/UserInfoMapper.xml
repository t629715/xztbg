  <select id="getByAccountMessage" resultType="com.fx.xzt.sys.model.UserInfoModel" parameterType="java.util.Map" >
  	select u.UserID as userIdString,l.UserName as userName,l.Status as status,u.NickName,u.RealName,
  	u.IDCard,b.BankName as bankName,ub.AccountNum as accountNum,u.PhoneNo,u.DeliveryAddress,
  	u.RegisterFrom,u.RegisterTime,a.BalanceAmt as balanceAmt,a.UBalanceAmt as uBalanceAmt,
  	a.UFrozenAmt as uFrozenAmt
	from User_Info u
	right join User_Login l on u.UserID = l.UserID
	<!-- left join User_BankCard ub on u.UserID = ub.UserID -->
	left join (select * from User_BankCard where State=1) ub on u.UserID = ub.UserID
	left join Bank b on ub.BankNo = b.BankNo
	left join User_Account a on a.UserID = u.UserID
	<where>
		<if test="userName != null and userName != '' ">
			and l.UserName = #{userName,jdbcType=VARCHAR}
		</if>
		<if test="status != null and status != '' ">
			and l.Status = #{status,jdbcType=SMALLINT}
		</if>
		<if test="realName != null and realName != '' ">
			and u.RealName= #{realName,jdbcType=VARCHAR}
		</if>
		<if test="accountNum != null and accountNum !='' ">
			and ub.AccountNum = #{accountNum,jdbcType=VARCHAR}
		</if>
		<if test="phone != null and phone != '' ">
			and u.PhoneNo = #{phone,jdbcType=VARCHAR}
		</if>
		<if test="registerFrom != null and registerFrom !='' ">
			and u.RegisterFrom = #{registerFrom,jdbcType=VARCHAR}
		</if>
		<if test="startTime !=null and startTime !='' ">
			and u.RegisterTime &gt;=#{startTime,jdbcType=TIMESTAMP}
		</if>
		<if test="endTime !=null and endTime != '' ">
			and u.RegisterTime &lt;=#{endTime,jdbcType=TIMESTAMP}
		</if>
	</where>
	order by RegisterTime DESC
  </select>
